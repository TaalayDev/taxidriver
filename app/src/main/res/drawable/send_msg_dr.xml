<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="#00FFFF00" android:dashWidth="1dp" />
            <solid android:color="@color/mapbox_blue" />
            <corners android:bottomLeftRadius="100dp" android:bottomRightRadius="0dp" android:topLeftRadius="100dp" android:topRightRadius="100dp" />
        </shape>
    </item>
</selector>